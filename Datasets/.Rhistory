library(agriTutorial)
library(data.table)
library(dplyr)
data(sorghum)
sorghum
sorghum <- sorghum[1:5]
names(sorghum) <- c("y", "gen", "rep", "week", "plot")
setcolorder(sorghum, c("week", "gen", "rep", "plot", "y"))
levels(sorghum$gen) <- c("A", "B", "C", "D")
sorghum <- sorghum[order(sorghum$week, sorghum$rep),]
sorghum$row <- as.numeric(rep(c(1,3,4,2,
2,3,1,4,
1,2,3,4,
1,4,2,3,
4,3,2,1), 5))
sorghum$col <- as.numeric(sorghum$rep)
sorghum$rep  <- sorghum$rep  %>% paste0("rep",.)  %>% as.factor
sorghum <- sorghum[order(sorghum$week, sorghum$col, sorghum$row),]
sorghum$plot <- rep(seq(1:20),5) %>% paste0("plot",.) %>% as.factor
head(sorghum)
repmes <- as.data.table(sorghum)
fwrite(repmes, sep="\t", "D:/RKurse/Dokumentation/crashcouRse/datasets/sorghum repmes.txt")
save(repmes,        file="D:/RKurse/Dokumentation/crashcouRse/datasets/sorghum repmes.rda")
repmes$row <- NULL
repmes$col <- NULL
fwrite(repmes, "D:/Hohenheim/R-SAS.Introductory.Courses/Datasets/06 sorghum repeated measures.txt", sep="\t")
